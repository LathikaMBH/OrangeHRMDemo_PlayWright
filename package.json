{
  "name": "la-playwright-taf",
  "version": "1.0.0",
  "description": "Playwright TypeScript Automation Framework with AI Agent",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:safari": "playwright test --project=webkit",
    "test:mobile": "playwright test --project=mobile-chrome",
    "test:report": "playwright show-report",
    "test:allure": "allure generate reports/allure-results -o reports/allure-report --clean && allure open reports/allure-report",
    "ai:analyze": "npx ts-node ai-agent/src/cli.ts analyze",
    "ai:generate": "npx ts-node ai-agent/src/cli.ts generate",
    "ai:improve": "npx ts-node ai-agent/src/cli.ts improve",
    "ai:fix-flaky": "ts-node ai-agent/src/cli.ts fix-flaky",
    "ai:enhance-reports": "ts-node ai-agent/src/cli.ts enhance-reports",
    "ai:generate-helpers": "ts-node ai-agent/src/cli.ts generate-helpers",
    "ai:optimize-framework": "ts-node ai-agent/src/cli.ts optimize-framework",
    "ai:create-page-objects": "ts-node ai-agent/src/cli.ts create-page-objects",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write src tests"
  },
  "keywords": [
    "playwright",
    "typescript",
    "automation",
    "testing",
    "ai"
  ],
  "author": "LathikaMBH",
  "license": "ISC",
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@faker-js/faker": "^8.3.1",
    "@octokit/rest": "^20.0.2",
    "@playwright/test": "^1.40.0",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "allure-commandline": "^2.25.0",
    "allure-playwright": "^2.10.0",
    "commander": "^11.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "faker": "^6.6.6",
    "fs-extra": "^11.3.1",
    "glob": "^11.0.3",
    "inquirer": "^9.2.12",
    "moment": "^2.29.4",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0",
    "winston": "^3.11.0"
  }
}
